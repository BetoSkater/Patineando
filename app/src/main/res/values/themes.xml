<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Patineando" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/principalNavajoWhite</item>
        <item name="colorPrimaryVariant">@color/principalNavajoWhite</item>
        <item name="colorOnPrimary">@color/principalPurpura</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/principalNavajoWhite</item>
        <item name="colorSecondaryVariant">@color/principalBlanco</item>
        <item name="colorOnSecondary">@color/principalPurpura</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Patineando.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Patineando.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Patineando.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>